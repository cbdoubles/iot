{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport dblib from '@/dblib';\nimport PopupBox from '@/components/PopupBox.vue';\nexport default {\n  components: {\n    PopupBox\n  },\n  data() {\n    return {\n      product: [],\n      isLoading: false,\n      popupMessage: '',\n      popupVisible: false\n    };\n  },\n  mixins: [dblib],\n  methods: {\n    async takeProduct() {\n      await this.deleteProduct(this.product).then(await this.showPopup(`Go to Box ${this.product.box_number} to collect your item!`));\n      this.$router.push({\n        path: `/login`\n      });\n    },\n    async showPopup(message) {\n      // Access the showPopup method of the PopupBox component\n      await this.$refs.popupBox.showPopup(message);\n    }\n  },\n  computed: {\n    loadingImage() {\n      return require('@/assets/loading.gif');\n    }\n  },\n  async created() {\n    const productId = this.$route.params.pid;\n    try {\n      this.isLoading = true;\n      this.product = await this.getProduct(productId);\n      console.log(this.product);\n    } catch (error) {\n      console.error('Error fetching product:', error);\n    } finally {\n      this.isLoading = false;\n    }\n  }\n};","map":{"version":3,"names":["dblib","PopupBox","components","data","product","isLoading","popupMessage","popupVisible","mixins","methods","takeProduct","deleteProduct","then","showPopup","box_number","$router","push","path","message","$refs","popupBox","computed","loadingImage","require","created","productId","$route","params","pid","getProduct","console","log","error"],"sources":["C:\\Users\\20223065\\Documents\\django_be\\website\\src\\views\\ProductView.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <template v-if=\"isLoading\">\r\n            <img :src=loadingImage style=\"max-width: 40%;\">\r\n        </template>\r\n\r\n        <template v-else>\r\n            <div>\r\n                <div class=\"back-btn\" @click=\"$router.go(-1)\"> back </div>\r\n                <main class=\"container\">\r\n\r\n                    <!-- Left Column / Headphones Image -->\r\n                    <div class=\"left-column\">\r\n                        <img class=\"active\" :src=\"product.get_image\" alt=\"\">\r\n                    </div>\r\n\r\n\r\n                    <!-- Right Column -->\r\n                    <div class=\"right-column\">\r\n\r\n                        <!-- Product Description -->\r\n                        <div class=\"product-description\">\r\n                            <span> Category </span>\r\n                            <h1> {{ product.title }}</h1>\r\n                            <p> {{ product.description }}</p>\r\n                        </div>\r\n\r\n\r\n                        <!-- Product Pricing -->\r\n                        <div class=\"product-price\">\r\n                            <div style=\"center\">\r\n                                <button @click=\"takeProduct\" class=\"cart-btn\">Take Product</button>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </main>\r\n                <PopupBox ref=\"popupBox\" />\r\n            </div>\r\n        </template>\r\n    </div>\r\n</template>\r\n\r\n\r\n\r\n<script>\r\nimport dblib from '@/dblib'\r\nimport PopupBox from '@/components/PopupBox.vue';\r\n\r\n\r\nexport default ({\r\n    components: {\r\n        PopupBox,\r\n    },\r\n\r\n    data() {\r\n        return {\r\n            product: [],\r\n            isLoading: false,\r\n            popupMessage: '',\r\n            popupVisible: false,\r\n        }\r\n    },\r\n    mixins: [dblib],\r\n\r\n    methods: {\r\n        async takeProduct() {\r\n            await this.deleteProduct(this.product)\r\n            .then(await this.showPopup(`Go to Box ${this.product.box_number} to collect your item!`))\r\n            this.$router.push({path: `/login`});\r\n        },\r\n\r\n        async showPopup(message) {\r\n            // Access the showPopup method of the PopupBox component\r\n            await this.$refs.popupBox.showPopup(message);\r\n        },\r\n    },\r\n\r\n    computed: {\r\n        loadingImage() {\r\n            return require('@/assets/loading.gif')\r\n        },\r\n    },\r\n\r\n    async created() {\r\n        const productId = this.$route.params.pid\r\n        try {\r\n            this.isLoading = true;\r\n            this.product = await this.getProduct(productId);\r\n            console.log(this.product);          \r\n        } catch (error) {\r\n            console.error('Error fetching product:', error);\r\n        } finally {\r\n            this.isLoading = false;\r\n        }\r\n    },\r\n\r\n})\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n/* Basic Styling */\r\nhtml,\r\nbody {\r\n    height: 100%;\r\n    width: 100%;\r\n    margin: 0;\r\n    font-family: 'Roboto', sans-serif;\r\n}\r\n\r\n.container {\r\n    max-width: 1200px;\r\n    margin: 0 auto;\r\n    padding: 15px;\r\n    display: flex;\r\n}\r\n\r\n/* Columns */\r\n.left-column {\r\n    width: 65%;\r\n    position: relative;\r\n}\r\n\r\n.right-column {\r\n    width: 35%;\r\n    margin-top: 60px;\r\n}\r\n\r\n/* Left Column */\r\n.left-column img {\r\n    width: 100%;\r\n    position: absolute;\r\n    left: 0;\r\n    top: 0;\r\n    opacity: 0;\r\n    transition: all 0.3s ease;\r\n}\r\n\r\n.left-column img.active {\r\n    opacity: 1;\r\n}\r\n\r\n/* Product Description */\r\n.product-description {\r\n    border-bottom: 1px solid #E1E8EE;\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.product-description span {\r\n    font-size: 12px;\r\n    color: #358ED7;\r\n    letter-spacing: 1px;\r\n    text-transform: uppercase;\r\n    text-decoration: none;\r\n}\r\n\r\n.product-description h1 {\r\n    font-weight: 300;\r\n    font-size: 52px;\r\n    color: #43484D;\r\n    letter-spacing: -2px;\r\n}\r\n\r\n.product-description p {\r\n    font-size: 16px;\r\n    font-weight: 300;\r\n    color: #86939E;\r\n    line-height: 24px;\r\n}\r\n\r\n/* Product Color */\r\n.product-color {\r\n    margin-bottom: 30px;\r\n}\r\n\r\n.color-choose div {\r\n    display: inline-block;\r\n}\r\n\r\n.color-choose input[type=\"radio\"] {\r\n    display: none;\r\n}\r\n\r\n.color-choose input[type=\"radio\"]+label span {\r\n    display: inline-block;\r\n    width: 40px;\r\n    height: 40px;\r\n    margin: -1px 4px 0 0;\r\n    vertical-align: middle;\r\n    cursor: pointer;\r\n    border-radius: 50%;\r\n    border: 2px solid #FFFFFF;\r\n    box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.33);\r\n}\r\n\r\n.color-choose input[type=\"radio\"]#red+label span {\r\n    background-color: #C91524;\r\n}\r\n\r\n.color-choose input[type=\"radio\"]#blue+label span {\r\n    background-color: #314780;\r\n}\r\n\r\n.color-choose input[type=\"radio\"]#black+label span {\r\n    background-color: #323232;\r\n}\r\n\r\n\r\n/* Cable Configuration */\r\n.cable-choose {\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.cable-choose button {\r\n    border: 2px solid #E1E8EE;\r\n    border-radius: 6px;\r\n    padding: 13px 20px;\r\n    font-size: 14px;\r\n    color: #5E6977;\r\n    background-color: #fff;\r\n    cursor: pointer;\r\n    transition: all .5s;\r\n}\r\n\r\n.cable-choose button:hover,\r\n.cable-choose button:active,\r\n.cable-choose button:focus {\r\n    border: 2px solid #86939E;\r\n    outline: none;\r\n}\r\n\r\n.cable-config {\r\n    border-bottom: 1px solid #E1E8EE;\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.cable-config a {\r\n    color: #358ED7;\r\n    text-decoration: none;\r\n    font-size: 12px;\r\n    position: relative;\r\n    margin: 10px 0;\r\n    display: inline-block;\r\n}\r\n\r\n.cable-config a:before {\r\n    content: \"?\";\r\n    height: 15px;\r\n    width: 15px;\r\n    border-radius: 50%;\r\n    border: 2px solid rgba(53, 142, 215, 0.5);\r\n    display: inline-block;\r\n    text-align: center;\r\n    line-height: 16px;\r\n    opacity: 0.5;\r\n    margin-right: 5px;\r\n}\r\n\r\n/* Product Price */\r\n.product-price {\r\n    display: flex;\r\n    display: inline-block;\r\n}\r\n\r\n.product-price span {\r\n    font-size: 26px;\r\n    font-weight: 300;\r\n    color: #43474D;\r\n    margin-right: 20px;\r\n}\r\n\r\n.cart-btn {\r\n    display: inline-block;\r\n    background-color: #7DC855;\r\n    border-radius: 6px;\r\n    font-size: 16px;\r\n    color: #FFFFFF;\r\n    text-decoration: none;\r\n    padding: 12px 30px;\r\n    transition: all .5s;\r\n}\r\n\r\n.cart-btn:hover {\r\n    background-color: #64af3d;\r\n}\r\n\r\n.back-btn {\r\n    display: inline-block;\r\n    background-color: #000000;\r\n    border-radius: 6px;\r\n    font-size: 16px;\r\n    color: #FFFFFF;\r\n    text-decoration: none;\r\n    padding: 12px 30px;\r\n    transition: all .5s;\r\n}\r\n\r\n.back-btn:hover {\r\n    background-color: #000000;\r\n}\r\n</style>"],"mappings":";AA6CA,OAAOA,KAAI,MAAO,SAAQ;AAC1B,OAAOC,QAAO,MAAO,2BAA2B;AAGhD,eAAgB;EACZC,UAAU,EAAE;IACRD;EACJ,CAAC;EAEDE,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE,KAAK;MAChBC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE;IAClB;EACJ,CAAC;EACDC,MAAM,EAAE,CAACR,KAAK,CAAC;EAEfS,OAAO,EAAE;IACL,MAAMC,WAAWA,CAAA,EAAG;MAChB,MAAM,IAAI,CAACC,aAAa,CAAC,IAAI,CAACP,OAAO,EACpCQ,IAAI,CAAC,MAAM,IAAI,CAACC,SAAS,CAAE,aAAY,IAAI,CAACT,OAAO,CAACU,UAAU,wBAAwB,CAAC;MACxF,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAACC,IAAI,EAAG;MAAO,CAAC,CAAC;IACvC,CAAC;IAED,MAAMJ,SAASA,CAACK,OAAO,EAAE;MACrB;MACA,MAAM,IAAI,CAACC,KAAK,CAACC,QAAQ,CAACP,SAAS,CAACK,OAAO,CAAC;IAChD;EACJ,CAAC;EAEDG,QAAQ,EAAE;IACNC,YAAYA,CAAA,EAAG;MACX,OAAOC,OAAO,CAAC,sBAAsB;IACzC;EACJ,CAAC;EAED,MAAMC,OAAOA,CAAA,EAAG;IACZ,MAAMC,SAAQ,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,GAAE;IACvC,IAAI;MACA,IAAI,CAACvB,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACD,OAAM,GAAI,MAAM,IAAI,CAACyB,UAAU,CAACJ,SAAS,CAAC;MAC/CK,OAAO,CAACC,GAAG,CAAC,IAAI,CAAC3B,OAAO,CAAC;IAC7B,EAAE,OAAO4B,KAAK,EAAE;MACZF,OAAO,CAACE,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;IACnD,UAAU;MACN,IAAI,CAAC3B,SAAQ,GAAI,KAAK;IAC1B;EACJ;AAEJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}